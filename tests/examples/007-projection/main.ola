role Alice
role Bob
role Charlie

type foo = String

protocol handshake
  = Alice   ==> Bob     { syn(Int)
  . Bob     ==> Charlie { syn(Int)
  . Charlie ==> Bob     { synack((Int,Int))
  . Bob     ==> Alice   { synack((Int,Int,Int))
  . Alice   ==> Bob     { fin((Int,Int,Int))
  . Bob     ==> Charlie { fin((Int,Int,Int))
  . end
  }}}}}}

protocol forever
  = rec(x). Alice ==> Bob   { syn(Int)
          . Bob   ==> Alice { fin(Int).end
                            | restart(Unit).call(x)
          }}

