func printLn(str:String) -> Unit
{
  print(str)
; print(toString('\n'))

}

main()
{
  var foo : String = "Hello World"
; printLn("Example 3")
; printLn(!foo)
; set foo to if true
                  {
                    "Goodbye World"
                  } else
                  {
                    "Should not be printed"
                  }

; printLn(!foo)

}
