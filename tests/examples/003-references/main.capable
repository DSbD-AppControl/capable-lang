func printLn(str:String) -> Unit
{
  print(str)
; print(toString('\n'))

}

main (args : [String])
{
  var foo : String = "Hello World"
; printLn("Example 3")
; printLn(!foo)
; local s = if true
                  {
                    "Goodbye World"
                  } else
                  {
                    "Should not be printed"
                  }
; mut(foo,s)
; printLn(!foo)

}
