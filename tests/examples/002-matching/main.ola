func printLn(str:String) -> Unit
{
  print(str)
; print(toString('\n'))

}
func extract(x : <urgh:String|oops:Int>) -> String
{
  match x
  {
    when urgh(s)
    {
      s
    }
    when oops(i)
    {
      "Hello!"
    }
  }
}

main()
{
  local foo : <urgh:String|oops:Int> = tag[urgh]("Ola")
; printLn(extract(foo))

}
