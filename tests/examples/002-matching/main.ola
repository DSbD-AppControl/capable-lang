func printLn(str:String) -> Unit
{
  print(str)
; print(toString('\n'))

}
func extract(x : union { urgh:String; oops:Int} ) -> String
{
  match x
  {
    when urgh(s)
    {
      s
    }
    when oops(i)
    {
      "Hello!"
    }
  }
}

main()
{
  local foo : union { urgh:String ; oops:Int } = tag[urgh]("Ola")
; printLn(extract(foo))

}
