func printLn(str:String) -> Unit
{
  print(str)
; print(toString('\n'))

}
func extract(x : <String|Int>) -> String
{
  match x
  {
    when left(s)
    {
      s
    }
    when right(i)
    {
      "Hello!"
    }
  }
}

main()
{
  local foo : <String|Int> = left("Ola")
  in { printLn(extract(foo))
     }
}
