func printLn(str:String) -> Unit
{
  print(str)
; print(toString('\n'))

}

func foo(a : <String|Int>) -> &String
{
  var x : String = "Hello World"
; printLn(!x)
; set x to match a
          {
            when left(s)
            {
             s
            }
            when right(i)
            {
              "Given an int"
            }
          }
; x
}

main()
{

  local bar : <String|Int> = left("AA")
; printLn(!foo(bar))
; local bar : <String|Int> = right(1)
; printLn(!foo(bar))

}
