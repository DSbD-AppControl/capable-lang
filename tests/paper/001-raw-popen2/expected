\CapableKeyword{func} \CapableBound{printLn} (\CapableBound{str} : \CapableType{String}) -> \CapableType{Unit}
\{
  \CapableKeyword{print}(\CapableBound{str});
  \CapableKeyword{print}(\CapableKeyword{toString}(\CapableValue{'\textbackslash{}n'}))
\}
\CapableKeyword{type} \CapableBound{SubProcess} = \CapableType{struct} \{\CapableBound{writeTo} : \CapableType{PROC}; \CapableBound{readFrom} : \CapableType{PROC}\}
\CapableKeyword{func} \CapableBound{closeFHS} (\CapableBound{fhs} : \CapableType{\CapableBound{SubProcess}}) -> \CapableType{Unit}
\{
  \CapableKeyword{close}(\CapableKeyword{get}[writeTo](\CapableBound{fhs}));
  \CapableKeyword{close}(\CapableKeyword{get}[readFrom](\CapableBound{fhs}))
\}
\CapableBound{main} () -> \CapableType{Unit}
\{
  \CapableBound{printLn}(\CapableValue{"My First Program with files"});
  \CapableKeyword{local} \CapableBound{mfh} = \CapableKeyword{popen2}(\CapableValue{"python3 pingping.py"}) ;
  \CapableKeyword{match} \CapableBound{mfh}
  \{
    \CapableKeyword{when} \CapableBound{left}(\CapableBound{e})
    \{
      \CapableBound{printLn}(\CapableValue{"Error opening"})
    \}
    \CapableKeyword{when} \CapableBound{right}(\CapableBound{fhs})
    \{
      \CapableKeyword{match} \CapableKeyword{write}(\CapableKeyword{get}[writeTo](\CapableBound{fhs}), \CapableValue{"rr"})
      \{
        \CapableKeyword{when} \CapableBound{left}(\CapableBound{err})
        \{
          \CapableBound{printLn}(\CapableValue{"Error sending line."})
        \}
        \CapableKeyword{when} \CapableBound{right}(\CapableBound{res})
        \{
          \CapableKeyword{match} \CapableKeyword{read}(\CapableKeyword{get}[readFrom](\CapableBound{fhs}))
          \{
            \CapableKeyword{when} \CapableBound{left}(\CapableBound{err})
            \{
              \CapableBound{printLn}(\CapableValue{"Error reading line."})
            \}
            \CapableKeyword{when} \CapableBound{right}(\CapableBound{res})
            \{
              \CapableBound{printLn}(\CapableBound{res});
              \CapableKeyword{close}(\CapableKeyword{get}[writeTo](\CapableBound{fhs}));
              \CapableKeyword{close}(\CapableKeyword{get}[readFrom](\CapableBound{fhs}))
            \}
          \}
        \}
      \}
    \}
  \}
\}
