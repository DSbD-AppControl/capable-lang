role Alice
role Bob

type foo = String

type ping = union { ping : String }
type pong = union { pong : String }

type pongR = union { pongR : String
                   ; quit  : String
                   }



protocol pingpong = Alice ==> Bob   [ping] { ping(String)
                  . Bob   ==> Alice [pong] { pong(String)
                  . end }}

protocol pingpongR = rec(x)
                   . Alice ==> Bob   [ping] { ping(String)
                   . Bob   ==> Alice [pongR] { pongR(String) . call(x)
                                             | quit(String) . end
                                             }}

main()
{
  print("My First Program")
; print(toString('\n'))

}
